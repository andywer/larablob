{
    "name": "andywer/larablob",
    "description": "Local blob store for the famous Laravel PHP web framework",
    "keywords": ["laravel", "blob", "store", "file", "upload"],
    "authors": [
        {
            "name": "Andy Wermke",
            "email": "andy@dev.next-step-software.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "rhumsaa/uuid": "~2.8"
    },
    "require-dev": {
        "orchestra/testbench": "3.0.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-0": {
            "Larablob": "src/"
        }
    },
    "minimum-stability": "stable"
}
